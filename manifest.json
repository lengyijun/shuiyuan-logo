{
  "name": "shuiyuan logo",
  "version": "1.1",
  "manifest_version": 3,
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "css": ["style.css"]
    }
  ],
  "permissions": ["favicon"],
  "web_accessible_resources": [
    {
      "resources": ["_favicon/*"],
      "matches": ["<all_urls>"]
    }
  ],
  "icons": {
    "32": "Untitled.png",
    "72": "Untitled.png",
    "128": "Untitled.png",
    "512": "Untitled.png"
  }

}
